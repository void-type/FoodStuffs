{
  "name": "clientapp",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "dev": "run-p type-check dev-only",
    "dev-only": "vite",
    "watch": "run-p type-check watch-only",
    "watch-only": "vite build --watch --mode development",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier ./src --write",
    "generate-api-client": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 sta generate -p https://localhost:5001/swagger/v1/swagger.json -o ./src/api/ --modular --responses --extract-request-params"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@tiptap/extension-list-item": "^2.11.7",
    "@tiptap/extension-typography": "^2.11.7",
    "@tiptap/pm": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "@tiptap/vue-3": "^2.11.7",
    "@vueuse/core": "^13.0.0",
    "bootstrap": "^5.3.4",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.17.30",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "npm-check-updates": "^17.1.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "swagger-typescript-api": "^13.0.28",
    "typescript": "~5.8.2",
    "vite": "^6.2.5",
    "vue-tsc": "^2.2.8"
  }
}
