{
  "Name": "FoodStuffs Dev",
  "BaseUrl": "https://localhost:5001",
  "ConnectionStrings": {
    "xFoodStuffs": "Server=server;Database=FoodStuffs;User Id=User;Password=password1;",
    "FoodStuffs": "Server=(LocalDb)\\MSSQLLocalDB;Database=FoodStuffsDev;trusted_connection=true;"
  },
  "Authorization": {
    "Policies": {}
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\webAppLogs\\FoodStuffs.Web\\FoodStuffs.Web-Development_.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 60,
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 10000000,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}]{Scope:l} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}]{Scope:l} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
